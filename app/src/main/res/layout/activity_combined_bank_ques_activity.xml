<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context=".JobsBankQuesActivities.combined_bank_ques_activity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="15dp"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/one"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED BANK 2018"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/two"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED 4 BANK IT 2018"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/three"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED 5 BANK"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/four"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED 8 BANK SO 2018"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/five"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED 8 BANK SO 2019"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/six"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED BANK 2019"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_marginTop="15dp"

                app:cardCornerRadius="12dp"
                app:cardElevation="8dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:padding="10dp"
                    android:id="@+id/seven"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="match_parent">
                    <ImageView

                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_marginLeft="5dp"
                        android:layout_height="32dp"
                        android:src="@drawable/combined_bank"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="COMBINED 3 BANK SO"
                        android:textColor="?android:attr/textColorPrimary"
                        android:textSize="20sp"

                        />

                </LinearLayout>



            </androidx.cardview.widget.CardView>





        </LinearLayout>



    </LinearLayout>

</LinearLayout>